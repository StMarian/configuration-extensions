<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>DevTrends.ConfigurationExtensions</PackageId>
    <Authors>Paul Hiles</Authors>
    <Company>DevTrends</Company>
    <Description>Bind configuration to records and classes with parameterized (non parameterless) constructors.</Description>
    <RepositoryUrl>https://github.com/devtrends-org/configuration-extensions</RepositoryUrl>
    <Copyright>DevTrends</Copyright>
    <PackageTags>configuration</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="6.0.0" />
  </ItemGroup>

</Project>
